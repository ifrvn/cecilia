{
  "compilerOptions": {
    "target": "es2021",
    "module": "commonjs",
    "declaration": true,
    "composite": true,
    "incremental": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "moduleResolution": "node",
    "strictNullChecks": true,
    "jsx": "react-jsx",
    "jsxImportSource": "@satorijs/element",
    "types": [
      "yml-register/types"
    ]
  },
  "exclude": [
    "node_modules"
  ],
}
